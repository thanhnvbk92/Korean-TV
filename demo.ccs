.clear{clear:both;}a, a:visited{color:#007bc4;	text-decoration:none;	outline:none;}a:hover{	text-decoration:underline;}#rounded{	width:800px;	margin:20px auto;	text-align:left;}.container{	background-color:#FFFFFF;	padding:10px 20px 20px 20px;}
